<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Number Strings -->
    <string-array name="number_suffixes">
        <item>тыс.</item>      <!-- thousand -->
        <item>млн.</item>      <!-- million -->
        <item>млрд.</item>      <!-- billion -->
        <item>трлн.</item>      <!-- trillion -->
        <item>птлн.</item>      <!-- peta -->
        <item>зтлн.</item>      <!-- zetta -->
        <item>йтлн.</item>      <!-- yotta -->
        <item>ктлн.</item>      <!-- quetta -->
    </string-array>

    <!-- Duration Strings -->
    <plurals name="nanoseconds">
        <item quantity="one">наносекунда</item>
        <item quantity="few">наносекунды</item>
        <item quantity="other">наносекунд</item>
    </plurals>

    <plurals name="microseconds">
        <item quantity="one">микросекунда</item>
        <item quantity="few">микросекунды</item>
        <item quantity="other">микросекунд</item>
    </plurals>

    <plurals name="milliseconds">
        <item quantity="one">миллисекунда</item>
        <item quantity="few">миллисекунды</item>
        <item quantity="other">миллисекунд</item>
    </plurals>

    <plurals name="seconds">
        <item quantity="one">секунда</item>
        <item quantity="few">секунды</item>
        <item quantity="other">секунд</item>
    </plurals>

    <plurals name="minutes">
        <item quantity="one">минута</item>
        <item quantity="few">минуты</item>
        <item quantity="other">минут</item>
    </plurals>

    <plurals name="hours">
        <item quantity="one">час</item>
        <item quantity="few">часа</item>
        <item quantity="other">часов</item>
    </plurals>

    <plurals name="days">
        <item quantity="one">день</item>
        <item quantity="few">дня</item>
        <item quantity="other">дней</item>
    </plurals>

    <plurals name="weeks">
        <item quantity="one">неделя</item>
        <item quantity="few">недели</item>
        <item quantity="other">недель</item>
    </plurals>

    <plurals name="months">
        <item quantity="one">месяц</item>
        <item quantity="few">месяца</item>
        <item quantity="other">месяцев</item>
    </plurals>

    <plurals name="years">
        <item quantity="one">год</item>
        <item quantity="few">года</item>
        <item quantity="other">лет</item>
    </plurals>

    <!-- Relative Time Strings -->
    <plurals name="relative_nanoseconds">
        <item quantity="one">наносекунду</item>
        <item quantity="few">наносекунды</item>
        <item quantity="other">наносекунд</item>
    </plurals>

    <plurals name="relative_microseconds">
        <item quantity="one">микросекунду</item>
        <item quantity="few">микросекунды</item>
        <item quantity="other">микросекунд</item>
    </plurals>

    <plurals name="relative_milliseconds">
        <item quantity="one">миллисекунду</item>
        <item quantity="few">миллисекунды</item>
        <item quantity="other">миллисекунд</item>
    </plurals>

    <plurals name="relative_seconds">
        <item quantity="one">секунду</item>
        <item quantity="few">секунды</item>
        <item quantity="other">секунд</item>
    </plurals>

    <plurals name="relative_minutes">
        <item quantity="one">минуту</item>
        <item quantity="few">минуты</item>
        <item quantity="other">минут</item>
    </plurals>

    <plurals name="relative_hours">
        <item quantity="one">час</item>
        <item quantity="few">часа</item>
        <item quantity="other">часов</item>
    </plurals>

    <plurals name="relative_days">
        <item quantity="one">день</item>
        <item quantity="few">дня</item>
        <item quantity="other">дней</item>
    </plurals>

    <plurals name="relative_weeks">
        <item quantity="one">неделю</item>
        <item quantity="few">недели</item>
        <item quantity="other">недель</item>
    </plurals>

    <plurals name="relative_months">
        <item quantity="one">месяц</item>
        <item quantity="few">месяца</item>
        <item quantity="other">месяцев</item>
    </plurals>

    <plurals name="relative_years">
        <item quantity="one">год</item>
        <item quantity="few">года</item>
        <item quantity="other">лет</item>
    </plurals>

    <string name="now">сейчас</string>
    <string name="time_in">через %1$s</string>
    <string name="time_ago">%1$s назад</string>

    <!-- Distance -->
    <string name="m">м</string>
    <string name="km">км</string>
    <string name="ft">фт</string>
    <string name="mi">ми</string>

    <!-- Size Strings -->
    <string-array name="size_suffixes">
        <item>Б</item>
        <item>КБ</item>
        <item>МБ</item>
        <item>ГБ</item>
        <item>ТБ</item>
        <item>ПБ</item>
        <item>ЭБ</item>
    </string-array>
</resources>
