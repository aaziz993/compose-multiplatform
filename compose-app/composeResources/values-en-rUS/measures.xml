<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Number Strings -->
    <string-array name="number_suffixes">
        <item>K</item>      <!-- thousand -->
        <item>M</item>      <!-- million -->
        <item>B</item>      <!-- billion -->
        <item>T</item>      <!-- trillion -->
        <item>P</item>      <!-- peta -->
        <item>Z</item>      <!-- zetta -->
        <item>Y</item>      <!-- yotta -->
        <item>Q</item>      <!-- quetta -->
    </string-array>

    <!-- Duration Strings -->
    <plurals name="nanoseconds">
        <item quantity="one">nanosecond</item>
        <item quantity="other">nanoseconds</item>
    </plurals>

    <plurals name="microseconds">
        <item quantity="one">microsecond</item>
        <item quantity="other">microseconds</item>
    </plurals>

    <plurals name="milliseconds">
        <item quantity="one">millisecond</item>
        <item quantity="other">milliseconds</item>
    </plurals>

    <plurals name="seconds">
        <item quantity="one">second</item>
        <item quantity="other">seconds</item>
    </plurals>

    <plurals name="minutes">
        <item quantity="one">minute</item>
        <item quantity="other">minutes</item>
    </plurals>

    <plurals name="hours">
        <item quantity="one">hour</item>
        <item quantity="other">hours</item>
    </plurals>

    <plurals name="days">
        <item quantity="one">day</item>
        <item quantity="other">days</item>
    </plurals>

    <plurals name="weeks">
        <item quantity="one">week</item>
        <item quantity="other">weeks</item>
    </plurals>

    <plurals name="months">
        <item quantity="one">month</item>
        <item quantity="other">months</item>
    </plurals>

    <plurals name="years">
        <item quantity="one">year</item>
        <item quantity="other">years</item>
    </plurals>

    <!-- Relative Time Strings -->
    <plurals name="relative_nanoseconds">
        <item quantity="one">nanosecond</item>
        <item quantity="other">nanoseconds</item>
    </plurals>

    <plurals name="relative_microseconds">
        <item quantity="one">microsecond</item>
        <item quantity="other">microseconds</item>
    </plurals>

    <plurals name="relative_milliseconds">
        <item quantity="one">millisecond</item>
        <item quantity="other">milliseconds</item>
    </plurals>

    <plurals name="relative_seconds">
        <item quantity="one">second</item>
        <item quantity="other">seconds</item>
    </plurals>

    <plurals name="relative_minutes">
        <item quantity="one">minute</item>
        <item quantity="other">minutes</item>
    </plurals>

    <plurals name="relative_hours">
        <item quantity="one">hour</item>
        <item quantity="other">hours</item>
    </plurals>

    <plurals name="relative_days">
        <item quantity="one">day</item>
        <item quantity="other">days</item>
    </plurals>

    <plurals name="relative_weeks">
        <item quantity="one">week</item>
        <item quantity="other">weeks</item>
    </plurals>

    <plurals name="relative_months">
        <item quantity="one">month</item>
        <item quantity="other">months</item>
    </plurals>

    <plurals name="relative_years">
        <item quantity="one">year</item>
        <item quantity="other">years</item>
    </plurals>

    <string name="now">now</string>
    <string name="time_in">in %1$s</string>
    <string name="time_ago">%1$s ago</string>

    <!-- Distance -->
    <string name="m">m</string>
    <string name="km">km</string>
    <string name="ft">ft</string>
    <string name="mi">mi</string>

    <!-- Size Strings -->
    <string-array name="size_suffixes">
        <item>B</item>
        <item>KB</item>
        <item>MB</item>
        <item>GB</item>
        <item>TB</item>
        <item>PB</item>
        <item>EB</item>
    </string-array>
</resources>
