<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Number Strings -->
    <string-array name="number_suffixes">
        <item>хаз.</item>      <!-- thousand -->
        <item>млн.</item>      <!-- million -->
        <item>мррд.</item>      <!-- billion -->
        <item>трлн.</item>      <!-- trillion -->
        <item>п.</item>      <!-- peta -->
        <item>з.</item>      <!-- zetta -->
        <item>й.</item>      <!-- yotta -->
        <item>к.</item>      <!-- quetta -->
    </string-array>

    <!-- Duration / Relative Time Strings -->
    <string name="now">хозир</string>
    <string name="time_in">дар доираи %1$s</string>
    <string name="time_ago">%1$s пеш</string>

    <plurals name="nanoseconds">
        <item quantity="one">наносония</item>
