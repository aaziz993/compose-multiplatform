CREATE TABLE IF NOT EXISTS KeyValue (key TEXT, value TEXT);

insert:
INSERT INTO KeyValue(key,value) VALUES(?, ?);

select:
SELECT KeyValue.* FROM KeyValue WHERE key = ?;

selectAll:
SELECT KeyValue.* FROM KeyValue;

delete:
DELETE FROM KeyValue WHERE key = ?;

deleteLike:
DELETE FROM KeyValue WHERE key LIKE ?;

deleteAll:
DELETE FROM KeyValue;

exists:
SELECT EXISTS(SELECT 1 FROM KeyValue WHERE key=? LIMIT 1);

count:
SELECT COUNT(*) FROM KeyValue;
